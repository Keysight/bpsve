    targetTags: {{ properties["GCP_MGMT_FIREWALL_RULE_NETWORK_TARGET_TAGS"] }}
    labels:
      owner: {{ properties["GCP_OWNER_TAG"].replace('.', '-').replace('@', '-') }}
